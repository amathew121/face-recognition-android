<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.piit.lbp.form"
      android:versionCode="1"
      android:versionName="1.0">
    <uses-sdk android:minSdkVersion="9" />
    <uses-permission android:name="android.permission.CAMERA"></uses-permission>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>

    <application android:icon="@drawable/icon" android:label="@string/app_name" android:screenOrientation="landscape">
        <activity android:name=".LBPForm"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            <intent-filter>
                <action android:name="com.piit.lbp.form.LBPFORM" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

       
        <activity android:name=".LBPFormAdd"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="com.piit.lbp.form.LBPFORMADD" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        
        <activity android:name=".LBPMatchImage"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="com.piit.lbp.form.LBPMATCHIMAGE" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        
        
        
       <activity android:name=".LBPSelectImages"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="com.piit.lbp.form.LBPSELECTIMAGES" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        
        <activity android:name=".LBPVectorCalculate"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="com.piit.lbp.form.LBPVECTORCALCULATE" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>        
        
        <activity android:name=".LBPRequestMatch"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="com.piit.lbp.form.LBPREQUESTMATCH" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        
        <activity android:name=".CameraCapture"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.media.action.IMAGE_CAPTURE" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
            <intent-filter>
                <action android:name="com.piit.lbp.form.CAMERACAPTURE" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        

        
        <activity android:name=".LBPReadData"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="com.piit.lbp.form.LBPREADDATA" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        
        <activity android:name=".FaceTest"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="com.piit.lbp.form.FACETEST" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        
        
     </application>
</manifest>